#include <stdio.h>
#include <string.h>
int main() {
    char line[200];
    printf("Enter a line: ");
    fgets(line, sizeof(line), stdin);
    line[strcspn(line, "\n")] = '\0';
    int len = strlen(line);https://maps.google.com/
    if (len >= 4 && line[0] == '/' && line[1] == '*' && 
        line[len-2] == '*' && line[len-1] == '/') {
        printf("Valid Comment: %s\n", line);
    } else {
        printf("Not a Comment: %s\n", line);
    }
    return 0;
}
